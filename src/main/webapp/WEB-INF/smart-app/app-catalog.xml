<smart-controller>

	<method name="add-shop">
		<bind name="shop" channel="app" class="com.clickntap.iolettore_webapp.bo.catalog.Shop" validation-group="create" scope="session"> ${shop.clear()} </bind>
		<exec> ${shop.create()} ${ws.setForm(this.param("form"))} </exec>
	</method>
	<method name="edit-shop">
		<bind name="shop" channel="app" class="com.clickntap.iolettore_webapp.bo.catalog.Shop" validation-group="update" scope="session"> ${shop.read()} </bind>
		<exec> ${shop.update()} ${ws.setForm(this.param("form"))} </exec>
	</method>
	<method name="delete-shop">
		<bind name="shop" channel="app" class="com.clickntap.iolettore_webapp.bo.catalog.Shop" scope="session"> ${shop.read()} </bind>
		<exec> ${shop.delete()} ${ws.setForm(this.param("form"))} </exec>
	</method>


